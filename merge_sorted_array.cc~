class Solution {
public:
  void merge(int A[], int m, int B[], int n) {
    int end = m + n - 1;
    int i = m - 1;
    int j = n - 1;
    while (i >= 0 && j >= 0) {
      if (A[i] > B[j]) {
	A[end] = A[i];
	--i;
      } else {
	A[end] = B[j];
	--j;
      }
      --end;
    }
    if (i < 0) {
      while (j >= 0) {
	A[end] = B[j];
	--end;
	--j;
      }
    }
  }
};
